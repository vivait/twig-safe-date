{
  "name" : "vivait/twig-safe-date",
  "description" : "A safe date function in twig that will not set null dates to today.",
  "type" : "library",
  "license" : "MIT",
  "authors" : [
    {
      "name" : "vivait",
      "email" : "helpdesk@vivait.co.uk"
    }
  ],
  "require" : {
    "php" : ">=8.3",
    "twig/twig" : "^3.10"
  },
  "autoload" : {
    "psr-4" : {
      "Vivait\\TwigSafeDate\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Vivait\\TwigSafeDateExtension\\Tests\\" : "tests/"
    }
  },
  "require-dev" : {
    "phpunit/phpunit" : "^9.5",
    "vimeo/psalm": "^5.13",
    "squizlabs/php_codesniffer": "^3.7"
  }
}
